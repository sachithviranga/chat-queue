{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ChatSettings": {
    "QueueMultiplier": 1.5,
    "AgentBaseConcurrency": 10,
    "InactiveAfterSeconds": 30,
    "InactiveAfterCount" : 3
  },
  "Quartz": {
    "InactiveCleanup": {
      "IntervalSeconds": 1,
      "Cron": null
    },
    "Dispatch": {
      "IntervalMilliseconds": 200,
      "Cron": null
    }
  }
}

